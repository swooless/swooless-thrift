{
  "name": "swooless/swooless-thrift",
  "description": "thrift server power by swoole",
  "type": "project",
  "authors": [
    {
      "name": "Alan Fu",
      "email": "ssisoo@live.cn"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "ext-redis": "*",
    "ext-sockets": "*",
    "symfony/console": "^4.1",
    "php-di/php-di": "^6.0",
    "monolog/monolog": "^1.24",
    "swooless/thrift-server": "^1.0",
    "swooless/registry": "^0.0.3",
    "swooless/config": "^1.0",
    "swooless/thrift-protocol-demo": "^1.0",
    "doctrine/annotations": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "swoole/ide-helper": "^4.3"
  },
  "repositories": {
    "protocol" : {
      "type": "github",
      "url": "git@github.com:swooless/thrift-protocol-demo.git"
    },
    "packagist": {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  }
}
